{
    "name": "feliximafidon/flysystem-google-drive",
    "description": "Flysystem adapter for Google Drive, adapted from nao-pon/flysystem-google-drive",
    "license": "MIT",
    "authors": [
        {
            "name": "Felix Imafidon",
            "email": "dancer.distal-0s@icloud.com"
        },
        {
            "name": "Naoki Sawada",
            "email": "hypweb@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "league/flysystem": "^1.1",
        "google/apiclient": "^2.0",
        "nao-pon/flysystem-cached-extra": "~1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0",
        "mockery/mockery": "0.9.*"
    },
    "autoload": {
        "psr-4": {
            "IF\\Flysystem\\GoogleDrive\\": "src/"
        }
    },
    "config": {
        "bin-dir": "bin"
    },
    "extra": {
        "branch-alias": {
            "dev-api_v2": "1.0.x-dev",
            "dev-master": "1.1.x-dev"
        },
        "laravel": {
            "providers": [
                "IF\\Flysystem\\GoogleDrive\\GoogleDriveServiceProvider"
            ]
        }
    }
}
